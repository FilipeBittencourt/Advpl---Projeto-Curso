	'1'																		, ; //ORDEM
	'ZBO_FILIAL+ZBO_VERSAO+ZBO_REVISA+ZBO_ANOREF+ZBO_PERIOD'				, ; //CHAVE
	'Versão+Revisão+Ano.Ref+Periodo'										, ; //DESCRICAO
	'Versão+Revisão+Ano.Ref+Periodo'										, ; //DESCSPA
	'Versão+Revisão+Ano.Ref+Periodo'										, ; //DESCENG
	'U'																		, ; //PROPRI
	''																		, ; //F3
	''																		, ; //NICKNAME
