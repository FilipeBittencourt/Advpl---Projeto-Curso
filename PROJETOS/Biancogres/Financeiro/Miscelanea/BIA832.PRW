User Function BIA832()
Local nValor := "0"

If SEA->EA_MODELO $ "31"				//PAGAMENTO DE TITULOS
	nValor	:=  STRZERO((SE2->E2_VALOR-SE2->E2_CNABDES+SE2->E2_CNABACR)*100,15)
Else
	nValor	:=  STRZERO((SE2->E2_SALDO-SE2->E2_CNABDES+SE2->E2_CNABACR)*100,15)
EndIf

Return(nValor)