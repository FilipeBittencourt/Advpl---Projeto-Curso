User Function BIA851()
Local nRet := ""

If Len(Alltrim(SE1->E1_NUM)) == 6
	nRet := SE1->E1_PREFIXO+ALLTRIM(SE1->E1_NUM)+SE1->E1_PARCELA
Else
	nRet := SE1->E1_PREFIXO+SUBSTR(SE1->E1_NUM,4,6)+SE1->E1_PARCELA
EndIf

Return(nRet)