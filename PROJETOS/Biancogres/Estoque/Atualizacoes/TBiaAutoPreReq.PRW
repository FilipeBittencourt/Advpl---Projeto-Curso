Method ProcBaixa() Class TBiaAutoPreReq

	Local oDlgProgresso
	Local aRet      
	Local oPreReq := ::oPreReq
	Local cUsuERP := ::oColetor:cUsuERP
	
	Define	MsDialog oDlgProgresso;
				Title "Aguarde...";
		    	From 100,100; 
		  		To 140,300; 
		  		Pixel; 
		    	Style DS_MODALFRAME

	@ 006,005 Say "Baixando Pré-Requisição... ";
			 Size 100,012;
			 Color CLR_BLUE;
			 Pixel Of oDlgProgresso;
			 Font ::oColetor:oBold11

	Activate MsDialog oDlgProgresso Centered On Init   ( aRet := oPreReq:BaixarReq(cUsuERP), oDlgProgresso:End() )
	
	If !( aRet[1] )
		::oColetor:DlgMensagem(aRet[2],"BAIXAR REQUISIÇÃO","ALERT")
	Else                                                            
		::oColetor:DlgMensagem("Pré Requisição baixada com sucesso!","BAIXAR REQUISIÇÃO","ALERT")
		
		::cGetReq	:= Space(9)
		::cGetNome	:= Space(30)
		::cGetData 	:= Space(10)
		::cGetCLVL	:= Space(4) 
		::oPreReq := TBiaPreRequisicao():New(::oColetor)
		::lLoadOk := .F.
		::lConfOk := .F.
		::oGetReq:SetFocus()	
	EndIf

Return( aRet[1] )