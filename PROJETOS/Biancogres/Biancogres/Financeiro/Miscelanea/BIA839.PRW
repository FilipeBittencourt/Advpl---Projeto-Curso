User Function BIA839()
Local nValor := ""

If SEA->EA_MODELO $ "31"				//PAGAMENTO DE TITULOS 
	If !Empty(Alltrim(SE2->E2_CODBAR))
			nValor := Substr(SE2->E2_CODBAR,10,10)
	Else
			nValor := Substr(SE2->E2_YLINDIG,38,10)
	EndIf
Else													
	nValor := StrZero((SE2->E2_SALDO*100),10)  
EndIf

Return(nValor)