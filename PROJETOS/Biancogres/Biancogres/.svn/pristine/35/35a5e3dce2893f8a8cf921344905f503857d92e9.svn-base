User Function BIAF147(lSchedule)
Local oObj := Nil

	Default lSchedule := .T.

	If lSchedule

		RpcSetType(3)
		RpcSetEnv("01", "01")
		
	EndIf
	
	oObj := TPoliticaCredito():New()
		
	oObj:UpdProcess()
		
	FreeObj(oObj)
	
	If lSchedule			
		
		RpcClearEnv()
		
	EndIf
		
Return()