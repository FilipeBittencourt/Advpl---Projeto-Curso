Static Function COMIS_EMAIL()
	Local lOk

	If cEmpAnt == "01"
		cRecebe		:= CEMAIL
		cRecebeCC	:= ""
		cAssunto	:= "BIANCOGRES - RELAÇÃO PESO DISPONIVEL POR CLIENTE "
	ElseIf cEmpAnt == "05"
		cRecebe   	:= CEMAIL
		cRecebeCC	:= "" 
		cAssunto	:= "INCESA - RELAÇÃO PESO DISPONIVEL POR CLIENTE "
	ElseIf cEmpAnt == "13"
		cRecebe   	:= CEMAIL
		cRecebeCC	:= "" 
		cAssunto	:= "MUNDI - RELAÇÃO PESO DISPONIVEL POR CLIENTE "
	EndIf

	If lDebugWF
		cRecebe		:= "tiago@facilesistemas.com.br"
		cRecebeCC	:= ""
	EndIf             

	lOk := U_BIAEnvMail(,cRecebe,cAssunto,C_HTML,,,.F.,cRecebeCC)

	If !lOK 
		EMA_N_CADAS("EMAIL INVÁLIDO: " + __TRAB_AUX->A1_COD+' - '+ __TRAB_AUX->A1_NOME + ' - ' + CEMAIL)
	EndIf

Return()

