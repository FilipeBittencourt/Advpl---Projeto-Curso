		cSql += "		,(SELECT COUNT(*) FROM SZN010 WITH (NOLOCK) WHERE ZN_FILIAL = '01' AND ZN_PEDIDO = C9_PEDIDO AND ZN_DATAINI <= CONVERT(varchar,GETDATE(),112) AND ZN_DATALIM >= CONVERT(varchar,GETDATE(),112) AND D_E_L_E_T_ = '' ) AS SC9SZN " + Enter
