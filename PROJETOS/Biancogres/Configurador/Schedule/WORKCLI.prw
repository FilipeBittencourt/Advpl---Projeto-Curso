				C_HTML += 'charset=iso-8859-1" /> <title>Untitled Document</title> <style type="text/css"> <!-- .style12 {font-size: 9px; } .style18 {font-size: 10} .style21 {color: #FFFFFF; font-size: 9px; } '
				C_HTML += '.style22 { 	font-size: 10pt; 	font-weight: bold; } .style35 {font-size: 10pt; } .style36 {font-size: 9pt; } .style39 {font-size: 12pt; } --> </style> </head>  '
				C_HTML += '<body> <table width="1044" border="1">   <tr>     '
				IF CEMPANT = "01"
					C_HTML += '<th width="236" rowspan="2" scope="col"> BIANCOGRES CERÂMICA SA </th>     '
				ELSEIF CEMPANT = "05"
					C_HTML += '<th width="236" rowspan="2" scope="col"> INCESA REVESTIMENTO LTDA </th>   '
				ELSEIF CEMPANT = "13"
					C_HTML += '<th width="236" rowspan="2" scope="col"> MUNDI LOGÍSTICA LTDA </th>   '
				ENDIF
				C_HTML += '<th width="520" rowspan="2" scope="col"> RELAÇÃO DE PESO DISPONÍVEL POR CLIENTE </th>     '
				C_HTML += '<td class="style12"><div align="right"> DATA EMISSÃO: '+ dtoC(DDATABASE) +' </div></td>   '
				C_HTML += '</tr>   <tr>     <td class="style12"><div align="right">HORA DA EMISSÃO: '+SUBS(TIME(),1,8)+' </div></td>   </tr> '
				C_HTML += '</table> '
				C_HTML += '<table width="1044" border="1"> '

				//CLIENTE
				C_HTML += '  <tr bgcolor="#FFFFFF"> '
				C_HTML += '    <th colspan="09" scope="col"><div align="left" class="style35">Cliente: '+ __TRAB_AUX->A1_COD+' - '+ __TRAB_AUX->A1_NOME +' - '+ __TRAB_AUX->A1_NREDUZ +' </div></th> '
				C_HTML += '  </tr> '

				// CABECALHO
				C_HTML += '  <tr bgcolor="#0066CC"> '
				C_HTML += '    <th width="20"	scope="col"><span class="style21"> Lc </span></th> '
				C_HTML += '    <th width="55" 	scope="col"><div align="center"><span class="style21"> Pedido </span></th> '
				C_HTML += '    <th width="55" 	scope="col"><div align="center"><span class="style21"> OC </span></th> '
				C_HTML += '    <th width="55" 	scope="col"><div align="center"><span class="style21"> Dt. Liberação </span></th> '
